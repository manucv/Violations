<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sipapp_project"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />
	<uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        
        <activity
            android:name="com.example.sipapp_project.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.sipapp_project.WelcomeActivity"
                  android:label="Bienvenido" >
        </activity>
        <activity android:name="com.example.sipapp_project.CategoriesActivity"
                  android:label="Categorias" >
        </activity>
        <activity android:name="com.example.sipapp_project.CategoryActivity"
                  android:label="Categoría" >
        </activity>
        <activity android:name="com.example.sipapp_project.EstablishmentActivity"
                  android:label="Establecimiento" >
        </activity>      
        <activity android:name="com.example.sipapp_project.ParkingActivity"
                  android:label="Parqueaderos" >
        </activity>   
        <activity android:name="com.example.sipapp_project.LocationActivity"
                  android:label="Ubicación" >
        </activity>
    </application>

</manifest>
